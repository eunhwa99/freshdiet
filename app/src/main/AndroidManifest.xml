<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.freshdiet">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Freshdiet">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Calendar"></activity>
        <activity android:name=".Piechart"></activity>
        <activity android:name=".Popup"
            android:theme="@android:style/Theme.Holo.Dialog.NoActionBar"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".TodoList"></activity>

    </application>

</manifest>